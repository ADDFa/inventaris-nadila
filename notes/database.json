[
    {
        "relation": {
            "users      - credentials"      : "one to one",
            "users      - building"         : "one to many",
            "users      - rooms"            : "one to many",
            "users      - items"            : "one to many",
            "building   - rooms"            : "one to many",
            "rooms      - items"            : "one to many",
            "rooms      - categories"       : "one to many",
            "rooms      - types"            : "one to many"
        },

        "database": {
            "tables": {
                "users": {
                    "id": "INT AUTO_INCREMENT PRIMARY_KEY",
                    "name": "VARCHAR(40)"
                },
                
                "credentials": {
                    "user_id": "INT FOREIGN_KEY",
                    "username": "VARCHAR(40)",
                    "password": "VARHCAR(255)"
                },

                "buildings": {
                    "id": "INT AUTO_INCREMENT PRIMARY_KEY",
                    "user_id":"INT FOREIGN_KEY",
                    "building_name": "VARCHAR(40)",
                    "bulding_size": "VARCHAR(10)",
                    "room_total": "INT 11",
                    "building_registration_date": "VARCHAR(40)",
                    "building_image": "VARCHAR(255)"
                },

                "rooms": {
                    "id": "INT AUTO_INCREMENT PRIMARY_KEY",
                    "user_id": "INT FOREIGN_KEY",
                    "building_id": "INT FOREIGN_KEY",
                    "room_name": "VARCHAR(40)",
                    "room_capacity": "INT 11",
                    "filed": "INT 11",
                    "available": "INT 11",
                    "room_size": "VARCHAR(10)",
                    "room_image": "VARCHAR(255)"
                },

                "items": {
                    "id":"INT AUTO_INCREMENT PRIMARY_KEY",
                    "user_id":"INT FOREIGN_KEY",
                    "building_id":"INT FOREIGN_KEY",
                    "category_id": "INT FOREIGN_KEY",
                    "type_id": "INT FOREIGN_KEY",
                    "item_name": "VARCHAR(40)",
                    "item_total": "INT 11"
                },

                "item_categories": {
                    "id": "INT AUTO_INCREMENT PRIMARY_KEY",
                    "category_name": "VARCHAR(40)"
                },

                "item_types": {
                    "id": "INT AUTO_INCREMENT PRIMARY_KEY",
                    "type_name": "VARCHAR(40)"
                }
            }
        }
    }
]